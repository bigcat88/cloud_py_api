{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base",
    ":disableDependencyDashboard"
  ],
  "ignorePaths": [
    "**/3rdparty/**",
    "**/docker/**",
    "**/docs/**",
    "**/lib/**",
    "**/proto/**",
    "**/src/**",
    "**/templates/**",
    "**/tests/**"
  ],
  "pre-commit": {
    "enabled": true
  }
}
